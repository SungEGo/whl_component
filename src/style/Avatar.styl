.whl_avatar {
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    font-family: Verdana, sans-serif;
    cursor: pointer;
    user-select: none;
    text-decoration: none;
    color: #fff;

    .usericon_img {
        border-radius: 50%;
        width: 100%;
        height: 100%;
    }

    .disablelink {
        pointer-events: none;
    }
}